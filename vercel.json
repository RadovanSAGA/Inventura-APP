{
  "version": 2,
  "name": "inventura",
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/health",
      "dest": "server/server.js"
    },
    {
      "src": "/",
      "dest": "server/server.js"
    }
  ]
}